{
  "expo": {
    "name": "Portfolio React Native",
    "slug": "portfolioreactnative",
    "privacy": "public",
    "description": "An application that show my portfolio.",
    "owner": "cuong",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mandus.portfolioreactnative",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.mandus.portfolioreactnative",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
